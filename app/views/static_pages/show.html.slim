h1.title
  = @static_page.title
.content
  = sanitize @static_page.content
.buttons
  - if @static_page_policy.edit?
    .edit
      = link_to "Edit", edit_static_page_path(@static_page)
  - if @static_page_policy.destroy?
    .destroy
      = link_to "Remove", static_page_path(@static_page), method: :delete
